<span class="c1">; The Boot Catalog has the only Checksum in the formal ISO specification.</span>
<span class="c1">; Here are the macro definitions to get NASM to calculate it for us!</span>
<span class="nf">ISO.Validation.Header</span><span class="w">    </span><span class="no">EQU</span><span class="w">    </span><span class="mi">01</span><span class="no">h</span><span class="w"></span>
<span class="nf">ISO.Validation.Signature</span><span class="w"> </span><span class="no">EQU</span><span class="w">    </span><span class="mi">0</span><span class="no">AA55h</span><span class="w"></span>

<span class="err">%</span><span class="nf">macro</span><span class="w">          </span><span class="no">ISO.Checksum</span><span class="w">    </span><span class="mi">2</span><span class="w"></span>
<span class="err">%</span><span class="nf">assign</span><span class="w">         </span><span class="no">Checksum</span><span class="w">        </span><span class="p">(</span><span class="err">%</span><span class="mi">2</span><span class="err">&lt;&lt;</span><span class="mi">8</span><span class="p">)</span><span class="w"> </span><span class="err">+</span><span class="w"> </span><span class="no">ISO.Validation.Header</span><span class="w"> </span><span class="err">+</span><span class="w"> </span><span class="no">ISO.Validation.Signature</span><span class="w"></span>
<span class="err">%</span><span class="nf">strlen</span><span class="w">         </span><span class="no">ID.Length</span><span class="w">       </span><span class="err">%</span><span class="mi">1</span><span class="w"></span>
<span class="err">%</span><span class="nf">assign</span><span class="w">         </span><span class="no">ID.Pos</span><span class="w">          </span><span class="mi">1</span><span class="w"></span>
<span class="err">%</span><span class="na">rep</span><span class="w"> </span><span class="nf">ID.Length</span><span class="w"></span>
<span class="err">%</span><span class="nf">substr</span><span class="w">         </span><span class="no">ID.High</span><span class="w">         </span><span class="err">%</span><span class="mi">1</span><span class="w"> </span><span class="no">ID.Pos</span><span class="err">+</span><span class="mi">1</span><span class="w"></span>
<span class="err">%</span><span class="nf">substr</span><span class="w">         </span><span class="no">ID.Low</span><span class="w">          </span><span class="err">%</span><span class="mi">1</span><span class="w"> </span><span class="no">ID.Pos</span><span class="err">+</span><span class="mi">0</span><span class="w"></span>
<span class="err">%</span><span class="nf">assign</span><span class="w">         </span><span class="no">Checksum</span><span class="w">        </span><span class="no">Checksum</span><span class="w"> </span><span class="err">+</span><span class="w"> </span><span class="p">((</span><span class="no">ID.High</span><span class="w"> </span><span class="err">&lt;&lt;</span><span class="w"> </span><span class="mi">8</span><span class="p">)</span><span class="w"> </span><span class="err">|</span><span class="w"> </span><span class="no">ID.Low</span><span class="p">)</span><span class="w"></span>
<span class="err">%</span><span class="nf">assign</span><span class="w">         </span><span class="no">ID.Pos</span><span class="w">          </span><span class="no">ID.Pos</span><span class="err">+</span><span class="mi">2</span><span class="w"></span>
<span class="err">%</span><span class="nf">endrep</span><span class="w"></span>
<span class="err">%00</span><span class="w">             </span><span class="nf">DW</span><span class="w">              </span><span class="p">(</span><span class="mi">0</span><span class="p">-</span><span class="no">Checksum</span><span class="p">)</span><span class="w"> </span><span class="err">&amp;</span><span class="w"> </span><span class="mi">0</span><span class="no">FFFFh</span><span class="w"></span>
<span class="err">%</span><span class="nf">endmacro</span><span class="w"></span>

<span class="err">%</span><span class="nf">macro</span><span class="w">          </span><span class="no">ISO.Validation</span><span class="w">  </span><span class="mi">2</span><span class="w"></span>
<span class="na">.Header</span><span class="w">         </span><span class="no">DB</span><span class="w">              </span><span class="no">ISO.Validation.Header</span><span class="w"></span>
