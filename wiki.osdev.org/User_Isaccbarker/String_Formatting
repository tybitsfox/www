<span class="w">        </span><span class="p">}</span><span class="w"></span>
<span class="w">        </span><span class="p">}</span><span class="w"></span>

<span class="w">        </span><span class="c1">// Add the current character to the buffer.</span>
<span class="w">        </span><span class="n">push_to_buf</span><span class="p">(</span><span class="n">stream</span><span class="p">,</span><span class="w"> </span><span class="n">cur</span><span class="p">);</span><span class="w"></span>

<span class="w">        </span><span class="k">goto</span><span class="w"> </span><span class="n">char_loop_end</span><span class="p">;</span><span class="w"></span>

<span class="nl">format_spec_end</span><span class="p">:</span><span class="w"></span>
<span class="w">        </span><span class="n">arg</span><span class="w"> </span><span class="o">=</span><span class="w"> </span><span class="sc">&#39;\0&#39;</span><span class="p">;</span><span class="w"></span>

<span class="nl">char_loop_end</span><span class="p">:</span><span class="w"></span>
<span class="w">        </span><span class="k">continue</span><span class="p">;</span><span class="w"></span>
<span class="w">    </span><span class="p">}</span><span class="w"></span>

<span class="w">    </span><span class="c1">// At the end, flush the buffer.</span>
<span class="w">    </span><span class="k">if</span><span class="w"> </span><span class="p">(</span><span class="n">print_stream_buf</span><span class="p">(</span><span class="n">stream</span><span class="p">)</span><span class="w"> </span><span class="o">!=</span><span class="w"> </span><span class="mi">0</span><span class="p">)</span><span class="w"> </span><span class="p">{</span><span class="w"></span>
<span class="w">        </span><span class="c1">// We didn&#39;t write the entire buffer, for some reason.</span>
<span class="w">        </span><span class="c1">// TODO: Decide what to do if we ever get here.</span>
<span class="w">    </span><span class="p">}</span><span class="w"></span>

<span class="w">    </span><span class="c1">// And of course reset the buffer index.</span>
<span class="w">    </span><span class="n">stream</span><span class="o">-&gt;</span><span class="n">buf_i</span><span class="w"> </span><span class="o">=</span><span class="w"> </span><span class="mi">0</span><span class="p">;</span><span class="w"></span>

<span class="w">    </span><span class="k">return</span><span class="w"> </span><span class="mi">0</span><span class="p">;</span><span class="w"></span>
<span class="p">}</span><span class="w"></span>
