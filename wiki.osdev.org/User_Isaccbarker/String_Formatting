<span class="w">        </span><span class="p">}</span><span class="w"></span>

<span class="w">        </span><span class="k">default</span><span class="o">:</span><span class="w"> </span><span class="p">{</span><span class="w"></span>
<span class="w">            </span><span class="k">if</span><span class="w"> </span><span class="p">(</span><span class="n">parse_mode</span><span class="w"> </span><span class="o">==</span><span class="w"> </span><span class="n">ARGUMENT</span><span class="p">)</span><span class="w"> </span><span class="p">{</span><span class="w"></span>
<span class="w">                </span><span class="n">arg</span><span class="w"> </span><span class="o">=</span><span class="w"> </span><span class="n">cur</span><span class="p">;</span><span class="w"></span>
<span class="w">                </span><span class="n">parse_mode</span><span class="w"> </span><span class="o">=</span><span class="w"> </span><span class="n">FORMAT_SPECIFIER</span><span class="p">;</span><span class="w"></span>

