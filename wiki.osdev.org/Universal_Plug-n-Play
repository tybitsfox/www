					<span class="nt">&lt;name&gt;</span>RequestedCount<span class="nt">&lt;/name&gt;</span>
					<span class="nt">&lt;direction&gt;</span>in<span class="nt">&lt;/direction&gt;</span>
					<span class="nt">&lt;relatedStateVariable&gt;</span>A_ARG_TYPE_Count<span class="nt">&lt;/relatedStateVariable&gt;</span>
				<span class="nt">&lt;/argument&gt;</span>
				<span class="nt">&lt;argument&gt;</span>
					<span class="nt">&lt;name&gt;</span>SortCriteria<span class="nt">&lt;/name&gt;</span>
					<span class="nt">&lt;direction&gt;</span>in<span class="nt">&lt;/direction&gt;</span>
					<span class="nt">&lt;relatedStateVariable&gt;</span>A_ARG_TYPE_SortCriteria<span class="nt">&lt;/relatedStateVariable&gt;</span>
				<span class="nt">&lt;/argument&gt;</span>
				<span class="nt">&lt;argument&gt;</span>
					<span class="nt">&lt;name&gt;</span>Result<span class="nt">&lt;/name&gt;</span>
					<span class="nt">&lt;direction&gt;</span>out<span class="nt">&lt;/direction&gt;</span>
					<span class="nt">&lt;relatedStateVariable&gt;</span>A_ARG_TYPE_Result<span class="nt">&lt;/relatedStateVariable&gt;</span>
				<span class="nt">&lt;/argument&gt;</span>
				<span class="nt">&lt;argument&gt;</span>
					<span class="nt">&lt;name&gt;</span>NumberReturned<span class="nt">&lt;/name&gt;</span>
					<span class="nt">&lt;direction&gt;</span>out<span class="nt">&lt;/direction&gt;</span>
					<span class="nt">&lt;relatedStateVariable&gt;</span>A_ARG_TYPE_Count<span class="nt">&lt;/relatedStateVariable&gt;</span>
