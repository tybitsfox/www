			<span class="nt">&lt;icon&gt;</span>
				<span class="nt">&lt;mimetype&gt;</span>image/jpeg<span class="nt">&lt;/mimetype&gt;</span>
				<span class="nt">&lt;height&gt;</span>48<span class="nt">&lt;/height&gt;</span>
				<span class="nt">&lt;width&gt;</span>48<span class="nt">&lt;/width&gt;</span>
				<span class="nt">&lt;depth&gt;</span>24<span class="nt">&lt;/depth&gt;</span>
				<span class="nt">&lt;url&gt;</span>/images/icon-48x48.jpg<span class="nt">&lt;/url&gt;</span>
			<span class="nt">&lt;/icon&gt;</span>
			<span class="nt">&lt;icon&gt;</span>
				<span class="nt">&lt;mimetype&gt;</span>image/jpeg<span class="nt">&lt;/mimetype&gt;</span>
				<span class="nt">&lt;height&gt;</span>120<span class="nt">&lt;/height&gt;</span>
				<span class="nt">&lt;width&gt;</span>120<span class="nt">&lt;/width&gt;</span>
				<span class="nt">&lt;depth&gt;</span>24<span class="nt">&lt;/depth&gt;</span>
				<span class="nt">&lt;url&gt;</span>/images/icon-120x120.jpg<span class="nt">&lt;/url&gt;</span>
			<span class="nt">&lt;/icon&gt;</span>
		<span class="nt">&lt;/iconList&gt;</span>
		<span class="nt">&lt;serviceList&gt;</span>
			<span class="nt">&lt;service&gt;</span>
				<span class="nt">&lt;serviceType&gt;</span>urn:schemas-upnp-org:service:ConnectionManager:1<span class="nt">&lt;/serviceType&gt;</span>
				<span class="nt">&lt;serviceId&gt;</span>urn:upnp-org:serviceId:ConnectionManager<span class="nt">&lt;/serviceId&gt;</span>
				<span class="nt">&lt;SCPDURL&gt;</span>/ConnectionManager.xml<span class="nt">&lt;/SCPDURL&gt;</span>
