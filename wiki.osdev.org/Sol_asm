<span class="w">    </span><span class="nl">Var1:</span><span class="w">    </span><span class="nf">db</span><span class="w"> </span><span class="err">?</span><span class="w"></span>
<span class="w">    </span><span class="nl">Var2:</span><span class="w">    </span><span class="nf">dw</span><span class="w"> </span><span class="err">?</span><span class="w"></span>
